<p id="notice"><%= notice %></p>

<h1>Dates</h1>

<table>
  <thead>
    <tr>
      <th>Date</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @dates.each do |date| %>
      <tr>
        <td><%= date.date %></td>
        <td><%= link_to 'Show', date %></td>
        <td><%= link_to 'Edit', edit_date_path(date) %></td>
        <td><%= link_to 'Destroy', date, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Date', new_date_path %>
